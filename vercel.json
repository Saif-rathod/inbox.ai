{
    "version": 2,
    "name": "inboxprism-frontend",
    "builds": [
        {
            "src": "frontend/package.json",
            "use": "@vercel/next",
            "config": {
                "projectSettings": {
                    "framework": "nextjs"
                }
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "frontend/$1"
        }
    ],
    "env": {
        "NEXT_PUBLIC_API_URL": "@api-url"
    },
    "functions": {
        "frontend/src/app/api/**/*.ts": {
            "runtime": "@vercel/node"
        }
    }
}
